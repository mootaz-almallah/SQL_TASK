FROM CourseAssignments CA\par
JOIN Courses C ON CA.course_id = C.course_id\par
JOIN Instructors I ON CA.instructor_id = I.instructor_id\par
WHERE CA.semester = 'Fall' AND CA.year = 2024;\par
