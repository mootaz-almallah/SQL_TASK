SELECT C.course_name, AVG(CASE \par
    WHEN E.grade = 'A' THEN \f1\rtlch\lang11265 4.0 \f0\ltrch\lang1033\par
    WHEN E.grade = 'B' THEN \f1\rtlch\lang11265 3.0 \f0\ltrch\lang1033\par
    WHEN E.grade = 'C' THEN \f1\rtlch\lang11265 2.0 \f0\ltrch\lang1033\par
    ELSE \f1\rtlch\lang11265 0 \f0\ltrch\lang1033 END) AS average_grade\par
FROM Courses C\par
JOIN Enrollments E ON C.course_id = E.course_id\par
WHERE C.course_name = 'Database Systems'\par
GROUP BY C.course_name;\par
